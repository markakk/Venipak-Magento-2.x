<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="unpkg-domain" type="host">https://unpkg.com</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="unpkg-domain" type="host">https://unpkg.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="terminalmappingjs-domain" type="host">https://www.terminalmappingjs.com</value>
                <value id="venipak-domain" type="host">https://osm.venipak.com</value>
                <value id="venipak" type="host">http://mano.venipak.lt</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="terminalmappingjs-domain" type="host">https://www.terminalmappingjs.com</value>
                <value id="arcgis-domain" type="host">https://geocode.arcgis.com</value>
                
            </values>
        </policy>
    </policies>
</csp_whitelist>